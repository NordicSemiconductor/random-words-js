{
  "name": "@nordicsemiconductor/random-words",
  "version": "0.0.0-development",
  "description": "Returns random 8-letter words from the Webster's Unabridged Dictionary",
  "main": "index.js",
  "bin": "cli.js",
  "typings": "index.d.ts",
  "scripts": {
    "test": "node tests.js",
    "prepare": "husky install"
  },
  "runkitExampleFilename": "example.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/random-words-js.git"
  },
  "keywords": [
    "random-words",
    "nrf-asset-tracker",
    "iot"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/random-words-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/random-words-js#readme",
  "dependencies": {
    "random-number-csprng": "1.0.2"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "devDependencies": {
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.20",
    "standard": "17.0.0"
  },
  "lint-staged": {
    "*.{yml,md,json}": [
      "prettier --write"
    ],
    "*.js": [
      "standard --fix"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/npm",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "files": [
    "package-lock.json",
    "README.md",
    "LICENSE",
    "index.js",
    "index.d.ts",
    "cli.js",
    "example.js",
    "words.js",
    "test.js"
  ]
}
